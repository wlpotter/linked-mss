                <handNote xml:id="p1handNote1" scope="major" script="syr-Syre" medium="unknown">
                  <desc>The writing is a fine, regular Estrangela of the<origDate notBefore="0500" notAfter="0600">vith cent.</origDate>
                  </desc>
                </handNote>
                <handNote xml:id="p1handNote2" scope="minor" script="syr-Syrj" medium="unknown">
                  <desc>See<ref target="#addition6">additions</ref>.</desc>
                </handNote>
