              <msItem xml:id="b5" n="12">
                <locus from="218a"/>
                <title xml:lang="en">Part of an Index of
                  the contents of the volume, from § <foreign xml:lang="syr">ܟܛ</foreign> to § <foreign xml:lang="syr">ܪܘ</foreign>
                </title>
              </msItem>
            </msItem>
