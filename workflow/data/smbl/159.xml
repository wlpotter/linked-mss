            <handDesc hands="4">
              <handNote xml:id="handNote1" scope="major" script="syr" medium="unknown">
                <desc>This volume is written in a good, regular hand of the<origDate notBefore="0800" notAfter="0900">ixth cent.</origDate>
                </desc>
              </handNote>
              <handNote xml:id="handNote2" scope="minor" script="syr" medium="red-green-and-blue-paint">
                <desc>
                  <ref target="#addition1">See additions.</ref>
                </desc>
              </handNote>
              <handNote xml:id="handNote3" scope="minor" script="syr" medium="unknown">
                <desc>Foll.<locus>105—112</locus>,  are paper leaves, written apparently by a hand of the<date notBefore="1000" notAfter="1200">xith or xiith cent.</date>
                </desc>
              </handNote>
              <handNote xml:id="handNote4" scope="minor" script="syr-Syrj" medium="unknown">
                <desc>
                  <ref target="#addition2">See additions.</ref>
                </desc>
              </handNote>
              <handNote xml:id="handNote5" scope="minor" script="syr" medium="unknown">
