              <supportDesc material="perg">
                <support>
                  <material>Vellum</material>
                </support>
                <extent>
                  <measure type="composition" unit="leaf" quantity="124">124 ff.</measure>
                  <dimensions type="leaf" unit="in">
                    <height>
                      <measure type="height" quantity="9.5" unit="in">9 1/2</measure>
                    </height>
                    <width>
