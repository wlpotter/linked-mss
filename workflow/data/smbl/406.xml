              <textLang mainLang="syr"/>
              <msItem xml:id="p1a1" n="1" defective="true">
                <locus from="1b" to="41"/>
                <title xml:lang="en">Part of a Chronicle, chiefly ecclesiastical</title>
                <note>
                <locus from="40" to="41">Foll. 40 and 41</locus> are small
