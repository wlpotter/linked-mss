                  <note>
                    <bibl>Opera, t. i., hom. 1</bibl>.</note>
                  <msItem xml:id="p4c1" n="5" defective="true">
                    <locus from="101b"/>
                    <title xml:lang="en">Contents</title>
                    <rubric xml:lang="syr">ܩܦܠܐܐ ܕܡܐܡܪܐ ܩܕܡܝܐ</rubric>
                  </msItem>
                  <msItem xml:id="p4c2" n="6">
                    <locus from="101b"/>
                    <title xml:lang="en">The passages of <ref>Scripture</ref> quoted in it</title>
                    <rubric xml:lang="syr">ܡ̈ܠܐ ܕܡܢ ܟ̈ܬܒܐ</rubric>
                  </msItem>
                  <msItem xml:id="p4c3" n="7">
                    <locus from="102b"/>
                    <author ref="http://syriaca.org/person/1016">Aitallāhā</author>
                    <title xml:lang="en">Another scholion, regarding the order of <ref>the homilies of <persName>Gregory Nazianzen</persName>
                      </ref>
