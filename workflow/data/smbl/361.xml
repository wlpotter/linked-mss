            <handDesc hands="4">
              <handNote xml:id="handNote1" medium="unknown" scope="major" script="syr-Syre">
                <desc>This volume is written in a good, regular Estrangela.</desc>
              </handNote>
              <handNote xml:id="handNote2" medium="unknown" scope="minor" script="syr">
                <desc>A somewhat later hand has written pieces on the first and last leaves of the volume.</desc>
              </handNote>
