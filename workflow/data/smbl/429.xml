            <handDesc hands="2">
              <handNote xml:id="handNote1" medium="unknown" scope="major" script="syr-Syre">
                <desc>This volume is written in a good, regular Estrangela of the <date>vith or viith cent.</date>
                </desc>
              </handNote>
              <handNote xml:id="handNote2" medium="unknown" scope="minor" script="ar-Syrc">
