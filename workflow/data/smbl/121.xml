            <msItem xml:id="a1" n="1">
              <locus from="1a" to="197a">Foll. 1a-197a</locus>
              <title type="supplied" ref="http://syriaca.org/work/180">The Ammonian sections and Eusebian canons</title>
              <note>They are marked in the ordinary way.</note>
            </msItem>
            <msItem xml:id="a2" n="2">
              <locus from="1a" to="197a">Foll. 1a-197a</locus>
              <title type="supplied" ref="http://syriaca.org/work/181">Gospel harmony</title>
              <note>They are marked at the foot of each page.</note>
            </msItem>
            <msItem xml:id="a3" n="3" defective="true">
              <locus from="1a" to="197a">Foll. 1a-197a</locus>
              <title ref="http://syriaca.org/work/71" type="supplied">The four Gospels, according to the Peshitta version</title>
