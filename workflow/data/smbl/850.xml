            <handDesc hands="5">
              <handNote xml:id="handNote1" medium="unknown" scope="major" script="syr">
                <desc>This volume is written in a small, current hand.</desc>
              </handNote>
              <handNote xml:id="handNote2" medium="unknown" scope="minor" script="mixed">
