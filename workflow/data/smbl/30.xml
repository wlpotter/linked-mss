                  (<date datingMethod="Seleucid" when-custom="0678">A. Gr. 678</date>), <persName ref="http://syriaca.org/person/13">Ephraim</persName> 
                  (<date datingMethod="Seleucid" when-custom="0684">A. Gr. 684</date>), and
